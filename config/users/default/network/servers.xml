<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<nodes>
    <server>
        <description>CentOS in VirtualBox</description>
        <favorite>true</favorite>
        <hostname>192.168.1.110</hostname>
        <name>CentOS 6</name>
    </server>
    <server>
        <description></description>
        <favorite>false</favorite>
        <hostname>localhost</hostname>
        <name>iMac</name>
    </server>
    <node type="server">
        <description>This is test node</description>
        <hostname>192.168.1.110</hostname>
        <favorite>false</favorite>
        <name>Test node</name>
        <services>
        	<service name="ICMP" monitoringClass="IcmpPing" pollingInterval="60" />
        	<service name="SSH" port="22" monitoringClass="PortPing" />
        	<service name="HTTP" monitoringClass="HttpPing">
        		<param key="httpping.interval" value="60" />
        		<param key="httpping.url" value="http://192.168.1.110/app" />
        		<param key="httpping.port" value="8080" />
        	</service>
        	<service name="SNMP">
        		<monitor type="HttpPing">
        			<param key="interval" value="60" />
	        		<param key="url" value="http://192.168.1.110/app" />
	        		<param key="port" value="8080" />
        		</monitor>
        		<param key="comunity" value="public" />
        	</service>
        </services>
    </node>
</nodes>
